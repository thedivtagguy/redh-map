{
  "version": 8,
  "name": "Cassini",
  "metadata": {
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=X72dC5GulJkxx67hT3SV"
    },
    "boat": {
      "type": "geojson",
      "data": "https://raw.githubusercontent.com/makinacorpus/cassini-gl-style/gh-pages/cassini-boat.geojson"
    },
    "sea": {
      "type": "geojson",
      "data": "https://raw.githubusercontent.com/makinacorpus/cassini-gl-style/gh-pages/cassini-sea.geojson"
    },
    "london-custom": {
      "type": "vector",
      "url": "pmtiles:///map/features.pmtiles"
    },
    "pois-geojson": {
      "type": "geojson",
      "data": "/map/data/pois.json"
    },
    "tracks-geojson": {
      "type": "geojson",
      "data": "/map/data/tracks.json"
    }
  },
  "sprite": "/map/sprite/sprite",
  "glyphs": "/map/font/chessyon/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-pattern": "background", "background-opacity": 1}
    },
    {
      "id": "boundary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": [
        "all",
        ["<=", "admin_level", 4],
        ["==", "$type", "LineString"],
        ["==", "maritime", 0]
      ],
      "layout": {"line-cap": "butt", "line-join": "bevel"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"base": 1.3, "stops": [[0, 2], [13, 4], [22, 6]]},
        "line-gap-width": 0,
        "line-opacity": 0.6,
        "line-dasharray": [0.6, 2]
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "minzoom": 11,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "grass", "park"]],
      "paint": {"fill-pattern": "landuse=grass", "fill-opacity": 0.9}
    },
    {
      "id": "park-fill",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "minzoom": 11,
      "maxzoom": 24,
      "filter": ["in", "class", "park"],
      "paint": {"fill-pattern": "landuse=grass", "fill-opacity": 0.9}
    },
    {
      "id": "landuse-building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "minzoom": 9,
      "maxzoom": 16,
      "filter": [
        "all",
        [
          "in",
          "class",
          "residential",
          "industrial",
          "hospital",
          "university",
          "college",
          "military",
          "school",
          "stadium",
          "retail",
          "commercial",
          "pitch",
          "railway",
          "cemetery"
        ]
      ],
      "paint": {
        "fill-pattern": "building2",
        "fill-opacity": {"stops": [[9, 0.05], [10, 0.1], [11, 0.2], [13, 0.5], [14, 0.6], [14.5, 0]]}
      }
    },
    {
      "id": "ww-river-border",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["==", "$type", "LineString"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-opacity": 0.8,
        "line-width": {"base": 1.4, "stops": [[8, 4], [20, 8]]}
      }
    },
    {
      "id": "ww-lake-border",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "lake", "river"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[8, 3], [20, 5]]},
        "line-offset": 0,
        "line-dasharray": [1, 1],
        "line-opacity": 0.8
      }
    },
    {
      "id": "ww-river-inner-clear",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["==", "$type", "LineString"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-opacity": 1,
        "line-width": {"base": 1.4, "stops": [[8, 2], [20, 5.5]]},
        "line-pattern": "background"
      }
    },
    {
      "id": "ww-river-inner-fill",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["==", "$type", "LineString"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": {"base": 1.4, "stops": [[8, 2], [20, 5.5]]},
        "line-pattern": "lake"
      }
    },
    {
      "id": "ww-lake-base",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "lake", "river", "ocean"]],
      "paint": {
        "fill-color": "#c5d4e0",
        "fill-opacity": 1
      }
    },
    {
      "id": "ww-lake-clear",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "lake", "river"]],
      "paint": {"fill-pattern": "background"}
    },
    {
      "id": "ww-lake-fill",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "lake", "river"]],
      "paint": {"fill-pattern": "lake"}

    },
    {
      "id": "ww-river-name",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "minzoom": 10,
      "maxzoom": 24,
      "filter": ["all", ["==", "$type", "LineString"]],
      "layout": {
        "visibility": "visible",
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-size": {"stops": [[10, 14], [14, 18], [18, 24]]},
        "text-font": ["Cheysson Italic Italic"],
        "text-letter-spacing": 0.15,
        "text-max-angle": 30
      },
      "paint": {
        "text-color": "#4a6fa5"
      }
    },
    {
      "id": "ocean-z6_1",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 6,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "ocean"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": {"stops": [[6, 6], [8, 10]]},
        "line-opacity": 0.8,
        "line-color": "#3b230b"
      }
    },
    {
      "id": "ocean-z6_1c",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 6,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "ocean"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": {"stops": [[6, 0], [8, 4]]},
        "line-opacity": 1,
        "line-pattern": "background"
      }
    },
    {
      "id": "ocean-z8_2",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 8,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "ocean"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": 2,
        "line-opacity": {"stops": [[0, 0], [7, 0.8]]},
        "line-color": "#3b230b",
        "line-dasharray": [4, 1, 3, 1]
      }
    },
    {
      "id": "ocean-c-clear",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 6,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "ocean"]],
      "paint": {"fill-pattern": "background"}
    },
    {
      "id": "ocean-z0-6_1",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 6,
      "filter": ["all", ["==", "class", "ocean"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": 3,
        "line-offset": -1,
        "line-opacity": 0.8
      }
    },
    {
      "id": "ocean-z0-8_2",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 8,
      "filter": ["all", ["==", "class", "ocean"]],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": 0.5,
        "line-offset": {"stops": [[0, 2.25], [6, 1.25], [8, -1]]},
        "line-gap-width": 0,
        "line-opacity": {"stops": [[0, 0], [2, 0.4], [7, 0.8]]},
        "line-color": "#3b230b",
        "line-dasharray": [4, 1, 3, 1]
      }
    },
    {
      "id": "boat",
      "type": "symbol",
      "source": "boat",
      "minzoom": 5,
      "maxzoom": 9,
      "layout": {
        "icon-image": "boat{boat}",
        "text-size": 16,
        "icon-size": {"stops": [[5, 0.1], [8, 1]]},
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "text-optional": false,
        "icon-allow-overlap": true,
        "icon-rotation-alignment": "map",
        "text-font": ["Cheysson Regular Regular"]
      },
      "paint": {"icon-translate-anchor": "map"}
    },
    {
      "id": "sea-name-low",
      "type": "symbol",
      "source": "sea",
      "minzoom": 0,
      "maxzoom": 8,
      "filter": ["all", ["==", "type", "2"]],
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["Cheysson Italic Italic"],
        "text-transform": "uppercase",
        "text-letter-spacing": 0.5
      },
      "paint": {
        "text-color": "#3b230b",
        "text-halo-color": "#ede3d7",
        "text-halo-width": 2
      }
    },
    {
      "id": "sea-name-hight",
      "type": "symbol",
      "source": "sea",
      "minzoom": 8,
      "maxzoom": 24,
      "filter": ["all", ["==", "type", "1"]],
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["Cheysson Italic Italic"],
        "text-transform": "uppercase",
        "text-letter-spacing": 1
      },
      "paint": {
        "text-color": "#3b230b",
        "text-halo-color": "#ede3d7",
        "text-halo-width": 2
      }
    },
    {
      "id": "water_name",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "water_name",
      "minzoom": 0,
      "maxzoom": 4,
      "layout": {
        "text-field": "{name:fr}",
        "text-font": ["Cheysson Italic Italic"],
        "text-size": {"stops": [[10, 16], [12, 28]]},
        "text-max-width": 4,
        "symbol-placement": "point",
        "text-letter-spacing": 0.2
      },
      "paint": {
        "text-color": "#3b230b",
        "text-halo-color": "#ede3d7",
        "text-halo-width": 2
      }
    },
    {
      "id": "road",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 7,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "trunk", "primary", "secondary", "tertiary"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[7, 1.5], [9, 2.5], [12, 3.5], [14, 4.5]]},
        "line-opacity": {"stops": [[7, 0.6], [14, 0.6], [15, 0.3], [16, 0]]}
      }
    },
    {
      "id": "road-brunnel",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 7,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "in",
          "class",
          "trunk",
          "primary",
          "secondary",
          "tertiary",
          "minor",
          "path"
        ],
        ["in", "brunnel", "bridge"]
      ],
      "layout": {"line-cap": "butt", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[7, 2], [9, 3], [14, 5]]},
        "line-opacity": {"stops": [[7, 0.6], [14, 0.6], [15, 0.3], [16, 0]]}
      }
    },
    {
      "id": "road_gap",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 7,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "any",
          ["in", "class", "trunk", "primary", "secondary", "tertiary"],
          ["all", ["in", "brunnel", "bridge"], ["in", "class", "minor", "path"]]
        ]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-width": {"stops": [[7, 0.8], [9, 1], [12, 1.5]]},
        "line-pattern": "background",
        "line-opacity": {"stops": [[7, 1], [14, 1], [15, 0.5], [16, 0]]}
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 10,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "minor", "service"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[10, 0.5], [13, 1]]},
        "line-opacity": {"stops": [[10, 0.2], [13, 0.4], [14, 0.4], [15, 0.2], [16, 0]]}
      }
    },
    {
      "id": "road-residential",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "class", "residential"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": 0.8,
        "line-opacity": {"stops": [[14, 0.4], [15, 0.2], [16, 0]]}
      }
    },
    {
      "id": "path-footway",
      "type": "line",
      "source": "london-custom",
      "source-layer": "park-paths",
      "minzoom": 15,
      "maxzoom": 24,
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#5c4a3a",
        "line-width": {"stops": [[15, 1], [18, 2]]},
        "line-opacity": 0.9,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "road_name",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "minzoom": 12,
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-size": {"stops": [[12, 12], [14, 14], [16, 16], [18, 20]]},
        "text-font": ["Cheysson Sans Caps Regular"],
        "text-transform": "uppercase"
      },
      "paint": {
        "text-color": "#3b230b",
        "text-halo-color": "#ede3d7",
        "text-halo-width": {"stops": [[12, 2], [15, 1], [16, 0]]},
        "text-opacity": {"stops": [[12, 0.6], [14, 0.8], [15, 0.9], [16, 1]]}
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "maxzoom": 24,
      "paint": {
        "fill-antialias": true,
        "fill-opacity": {"stops": [[14, 0], [15, 0.5]]},
        "fill-pattern": "building2"
      }
    },
    {
      "id": "construction-fill",
      "type": "fill",
      "source": "london-custom",
      "source-layer": "construction",
      "minzoom": 14,
      "maxzoom": 24,
      "paint": {
        "fill-antialias": true,
        "fill-opacity": {"stops": [[14, 0], [15, 0.5]]},
        "fill-pattern": "building2"
      }
    },
    {
      "id": "construction-outline",
      "type": "line",
      "source": "london-custom",
      "source-layer": "construction",
      "minzoom": 15,
      "maxzoom": 24,
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[15, 1], [17, 1.5]]},
        "line-dasharray": [3, 2],
        "line-opacity": {"stops": [[15, 0.5], [16, 0.7]]}
      }
    },
    {
      "id": "grass",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "grass"]],
      "paint": {
        "line-color": "#3b230b",
        "line-width": 1.5,
        "line-blur": 0.3,
        "line-opacity": {"stops": [[14, 0], [15, 1]]},
        "line-gap-width": 0,
        "line-pattern": "building2"
      }
    },
    {
      "id": "building-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "maxzoom": 24,
      "layout": {},
      "paint": {
        "line-color": "#3b230b",
        "line-width": 1.2,
        "line-blur": 1,
        "line-opacity": {"stops": [[14, 0], [15, 1]]}
      }
    },
    {
      "id": "railway-base",
      "type": "line",
      "source": "london-custom",
      "source-layer": "railways",
      "minzoom": 0,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "stops": [
            [11, "hsla(2, 26%, 35%, 1)"],
            [14, "hsla(10, 31%, 59%, 0.8)"],
            [16, "#1a1817"]
          ]
        },
        "line-width": {
          "base": 1.4,
          "stops": [
            [11, 0.5],
            [13, 1],
            [15, 1],
            [17, 1],
            [20, 1]
          ]
        },
        "line-opacity": {
          "stops": [
            [11, 0.8],
            [13, 0.9],
            [14, 0.95],
            [16, 0.9],
            [18, 0.85]
          ]
        }
      }
    },
    {
      "id": "railway-hatching",
      "type": "line",
      "source": "london-custom",
      "source-layer": "railways",
      "minzoom": 0,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "stops": [
            [15, "hsla(2, 26%, 35%, 1)"],
            [17, "#1a1817"]
          ]
        },
        "line-dasharray": [2, 2],
        "line-width": {
          "base": 1.4,
          "stops": [
            [15, 1.5],
            [17, 3.5],
            [20, 3.5]
          ]
        },
        "line-opacity": {
          "stops": [
            [15, 0.9],
            [17, 0.85],
            [19, 0.8]
          ]
        }
      }
    },
    {
      "id": "platforms-fill",
      "type": "fill",
      "source": "london-custom",
      "source-layer": "platforms",
      "minzoom": 0,
      "maxzoom": 24,
      "paint": {
        "fill-pattern": "station",
        "fill-opacity": {"stops": [[15, 0.8], [17, 0.85], [20, 0.9]]}
      }
    },
    {
      "id": "platforms-outline",
      "type": "line",
      "source": "london-custom",
      "source-layer": "platforms",
      "minzoom": 0,
      "maxzoom": 24,
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#3b230b",
        "line-width": {"stops": [[0, 0.3], [8, 0.5], [12, 0.7], [15, 1], [17, 1.5], [20, 2]]},
        "line-opacity": {"stops": [[0, 0], [8,0], [12, 0], [15, 1], [17, 1], [20,1]]}
      }
    },
    {
      "id": "building-3D-background",
      "type": "fill-extrusion",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "maxzoom": 24,
      "layout": {"visibility": "none"},
      "paint": {
        "fill-extrusion-height": {
          "property": "render_height",
          "type": "identity"
        },
        "fill-extrusion-base": {
          "property": "render_min_height",
          "type": "identity"
        },
        "fill-extrusion-pattern": "background"
      }
    },
    {
      "id": "building-3D-texture",
      "type": "fill-extrusion",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "maxzoom": 24,
      "layout": {"visibility": "none"},
      "paint": {
